{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://discord.com/*"
        },
        {
          "url": "https://cordapi.dolfi.es/api/v2/properties/*"
        }
      ]
    },
    "http:default",
    "log:default",
    "websocket:default"
  ]
}